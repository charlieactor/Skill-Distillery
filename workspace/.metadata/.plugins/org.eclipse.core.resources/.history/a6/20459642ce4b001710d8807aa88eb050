package labs;

public class AddressTest {
	public static void main(String[] args) {
		StringBuilder zip = new StringBuilder();
		int houseNum = -2;
		zip.append("80205");
		Address add1 = new Address(-10, "Ogden St", "Denver", "CO", zip);
		System.out.println(add1.houseNumber);
		System.out.println(add1.zipcode);
		
		add1.setHouseNumber();
	}
}
