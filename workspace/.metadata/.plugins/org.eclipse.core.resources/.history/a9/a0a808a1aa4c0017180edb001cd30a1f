package lordofobjects;

public class Level2 extends Enemies {

	public Level2(int health, int attack) {
		super(health, attack);
		// TODO Auto-generated constructor stub
	}

@Override
	
	public int attack() {
		int damage = 6 + ((int)(Math.random() * 3));
		double probability = Math.random();
		if (probability >= 0.4) {
			return damage;
		}
		else {
			damage = 0;
			return damage;
		}
	}

}
