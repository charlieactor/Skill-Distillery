package bingo;

import java.util.*;

public class Card implements Comparable {
	String[][] card = new String[5][5];

	public Card makeNewCard(){
		for (int i = 0; i < 5; i++){
			for (int j = 0; j < 5; j++) {
				if (i == 0) {
					String marker = Integer.toString(((int)Math.random()*15) + 1);
					List<String> column = new ArrayList<>();
					if (column.contains(marker)) {
						j = j - 1;
					}
					else {
						column.add(marker);
						card[i][j] = marker;
					}
				}
				else if (i == 1) {
					String marker = Integer.toString(((int)Math.random()*15) + 16);
					List<String> column = new ArrayList<>();
					if (column.contains(marker)) {
						j = j - 1;
					}
					else {
						column.add(marker);
						card[i][j] = marker;
					}
				}
				else if (i == 2) {
					String marker = Integer.toString(((int)Math.random()*15) + 31);
					List<String> column = new ArrayList<>();
					if (column.contains(marker)) {
						j = j - 1;
					}
					else {
						column.add(marker);
						card[i][j] = marker;
					}
				}
				else if (i == 3) {
					String marker = Integer.toString(((int)Math.random()*15) + 46);
					List<String> column = new ArrayList<>();
					if (column.contains(marker)) {
						j = j - 1;
					}
					else {
						column.add(marker);
						card[i][j] = marker;
					}
				}
				else if (i == 4) {
					String marker = Integer.toString(((int)Math.random()*15) + 61);
					List<String> column = new ArrayList<>();
					if (column.contains(marker)) {
						j = j - 1;
					}
					else {
						column.add(marker);
						card[i][j] = marker;
					}
				}
				
			}
		}

	return null;
}

}
