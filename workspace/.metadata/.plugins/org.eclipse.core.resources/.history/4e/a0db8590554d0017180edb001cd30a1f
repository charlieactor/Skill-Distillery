package examples;

public class Person {
    private String name;
    private int age;

    public Person(String n, int age;) throws InvalidDataException {
        if (n == null || n.equals("")) {
            throw new InvalidDataException();
        }
        name = n;
        this.age = age;
    }

    public String toString() {
        return "Person [name=" + name + "]";
    }
}
