package labs;

public class Bicycle extends Vehicle {
	int gears;

	public Bicycle(int price, int wheels, int gears) {
		super(price, wheels);
		this.gears = gears;
	}
	public int getGears() {
		return gears;
	}

	public void setGears(int gears) {
		this.gears = gears;
	}
	@Override
	public String toString() {
		return "Bicycle [gears=" + gears + "]";
	}

	
	
}
