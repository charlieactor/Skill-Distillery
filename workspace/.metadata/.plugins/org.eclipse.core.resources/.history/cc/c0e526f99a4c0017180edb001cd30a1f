package lordofobjects;

public class Male extends MainCharacter {
	int agility;

	public Male(int health, int attack, String name, int agility) {

		super(health, attack, name);
		this.agility = agility;
	}
	
@Override
	
	public int attack(String type) {
		int damage = 3 + ((int)(Math.random() * 3));
		double probability = Math.random();
		if (probability >= 0.5) {
			return damage;
		}
		else {
			damage = 0;
			return damage;
		}
	}
}
