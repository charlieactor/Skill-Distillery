package labs;

import java.util.ArrayList;
import java.util.List;

public class Numbers {
	public static void main(String[] args) {
		List<Double> numbers = new ArrayList<>();
		
		for (int i = 0; i < 10; i++) {
			numbers.add((Math.random() * 1000) / 1000);
		}
		for (int i = 0; i < numbers.size(); i++) {
			numbers.get(i);
		}
	}
}
