package labs;

public abstract class Shape {
		String color;
		
		Shape(Color color) {
			this.color = color;
		}
		
		Shape() {

		}
		
		public abstract double getArea();
		
		public String getColor() {
			return color;
		}

		public void setColor(String color) {
			this.color = color;
		}
		


}
