package labs;

public class EmpTest {
	public static void main(String[] args) {
		Employee emp1 = new Employee();
		emp1.firstName = "Alan";
		emp1.lastName = "Smith";
		emp1.salary = 90000;
		emp1.employeeID = 2526;
		emp1.display();
		
		Employee emp2 = new Employee();
		emp2.firstName = "Sally";
		emp2.lastName = "Yates";
	}
}
