package labs;

public class AddressTest {
	public static void main(String[] args) {
		Address add1 = new Address(2431, "Ogden St", "Denver", "CO", "80205");
		
		StringBuilder zip = new StringBuilder();
		int houseNum = -2;
		zip.append("80205");
		
		System.out.println(add1);
		System.out.println(add1.houseNumber);
		System.out.println(add1.zipcode);
		
		add1.setHouseNumber(houseNum);
		add1.setZipcode(zip);
	}
}
