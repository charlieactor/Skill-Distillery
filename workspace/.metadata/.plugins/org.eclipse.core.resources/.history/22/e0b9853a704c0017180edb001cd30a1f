package labs;

public class Automobile extends Vehicle {
	protected String make, model;
	protected int year;
	
	Automobile(int price, int wheels, String make, String model, int year) {
		super(price, wheels);
		this.make = make;
		this.model = model;
		this.year = year;
	}
	Automobile() {

	}
	
	public double calculateRegistrationFee() {
		double fee = this.getPurchasePrice() * .01;
		
	}
	public String getMake() {
		return make;
	}
	public void setMake(String make) {
		this.make = make;
	}
	public String getModel() {
		return model;
	}
	public void setModel(String model) {
		this.model = model;
	}
	public int getYear() {
		return year;
	}
	public void setYear(int year) {
		this.year = year;
	}
	@Override
	public String toString() {
		return "Automobile [make=" + make + ", model=" + model + ", year=" + year + ", purchasePrice=" + purchasePrice
				+ ", numWheels=" + numWheels + "]";
	}
	
}
