package battleship;

public class GameDriver {
	public static void main(String[] args) {
		
		Ship[] shipArr = new Ship[7];
		Ship shipOne = new Ship("Ship One", 5, 5);
		
		for (int i = 0; i < shipOne.getLength(); i++) {
			shipArr[i] = shipOne;
			System.out.println(shipArr[i].toString());
		}
		
		System.out.println(shipAr[5]);
		System.out.println(shipAr[6]);
		System.out.println(shipAr[7]);
		
		
		
	}
}
