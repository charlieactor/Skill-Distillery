package lordofobjects;

public class Level1 extends Enemies {

	public Level1(int health, int attack) {
		super(health, attack);
		// TODO Auto-generated constructor stub
	}
	
	public int attack() {
		int damage = 3 + ((int)(Math.random() * 3));
		double probability = Math.random();
		if (probability >= 0.5) {
			return damage;
		}
		else {
			damage = 0;
			return damage;
		}
		
	}

}
