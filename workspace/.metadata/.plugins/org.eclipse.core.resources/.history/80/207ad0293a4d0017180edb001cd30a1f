package labs;

public class ShapeTester {

	public static void main(String[] args) {
		Drawable[] shapeArr = new Shape[3];

		shapeArr[0] = new Rectangle(Color.GREEN, 4, 6);
		shapeArr[1] = new Text("Some text");
		shapeArr[2] = new Circle(Color.ORANGE, 3);

		for (Drawable shape : shapeArr) {
			shape.draw();
		}
	}
}
