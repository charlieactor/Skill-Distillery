package lordofobjects;

public abstract class MainCharacter {
	int health, attack;
	String name;
	
	public int attack() {
		return 10;
	}

	public MainCharacter(int health, int attack, String name) {
		super();
		this.health = health;
		this.attack = attack;
		this.agility = agility;
		this.name = name;
	}
}
